#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Russell/Downloads/AI-Projects/Mesza-AI-clean/node_modules/.pnpm/convex@1.19.5_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/convex/bin/node_modules:/mnt/c/Users/Russell/Downloads/AI-Projects/Mesza-AI-clean/node_modules/.pnpm/convex@1.19.5_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/convex/node_modules:/mnt/c/Users/Russell/Downloads/AI-Projects/Mesza-AI-clean/node_modules/.pnpm/convex@1.19.5_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules:/mnt/c/Users/Russell/Downloads/AI-Projects/Mesza-AI-clean/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Russell/Downloads/AI-Projects/Mesza-AI-clean/node_modules/.pnpm/convex@1.19.5_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/convex/bin/node_modules:/mnt/c/Users/Russell/Downloads/AI-Projects/Mesza-AI-clean/node_modules/.pnpm/convex@1.19.5_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/convex/node_modules:/mnt/c/Users/Russell/Downloads/AI-Projects/Mesza-AI-clean/node_modules/.pnpm/convex@1.19.5_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules:/mnt/c/Users/Russell/Downloads/AI-Projects/Mesza-AI-clean/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../convex/bin/main.js" "$@"
else
  exec node  "$basedir/../convex/bin/main.js" "$@"
fi
